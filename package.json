    "@react-navigation/stack": "^7.1.1",
    "@types/react": "^18.2.12",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-gesture-handler": "2.8.0",
    "react-native-gradle-plugin": "^0.71.19",
